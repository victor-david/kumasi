<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="StandardPathStyle" TargetType="Path">
        <Setter Property="Fill" Value="{StaticResource StandardIconBrush}"/>
        <Setter Property="Stretch" Value="Uniform"/>
        <Setter Property="Height" Value="18"/>
        <Setter Property="Width" Value="18"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
    </Style>

    <Style x:Key="MediumPathStyle" TargetType="Path" BasedOn="{StaticResource StandardPathStyle}">
        <Setter Property="Height" Value="14"/>
        <Setter Property="Width" Value="14"/>
    </Style>

    <Style x:Key="SmallPathStyle" TargetType="Path" BasedOn="{StaticResource StandardPathStyle}">
        <Setter Property="Height" Value="10"/>
        <Setter Property="Width" Value="10"/>
    </Style>


    <Geometry x:Key="MusicNoteGeometryKey">M17 9V12H14V14H17V17H19V14H22V12H19V9H17M9 3V13.55C8.41 13.21 7.73 13 7 13C4.79 13 3 14.79 3 17S4.79 21 7 21 11 19.21 11 17V7H15V3H9Z</Geometry>

    <Path x:Key="AppPathIconKey" x:Shared="False" Style="{StaticResource StandardPathStyle}" Data="{StaticResource MusicNoteGeometryKey}"/>

</ResourceDictionary>