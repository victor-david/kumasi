    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Dictionary/Brush.xaml"/>
                <ResourceDictionary Source="Resources/Dictionary/Path.xaml"/>
                <ResourceDictionary Source="Resources/Dictionary/Images.xaml"/>
                <ResourceDictionary Source="Resources/Dictionary/Button.xaml"/>
