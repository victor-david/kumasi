<Application 
    x:Class="Restless.App.Tambala.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Dictionary/Values.xaml"/>
                <ResourceDictionary Source="Resources/Dictionary/Brushes.xaml"/>
                <ResourceDictionary Source="Resources/Dictionary/Images.xaml"/>
                <ResourceDictionary Source="Resources/Dictionary/ControlDefault.xaml"/>
                <ResourceDictionary Source="Resources/Dictionary/Button.xaml"/>
                <ResourceDictionary Source="Resources/Dictionary/TextBlock.xaml"/>
                <ResourceDictionary Source="Resources/Dictionary/ItemsControl.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Tambala.Controls;component/Themes/Generic.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
